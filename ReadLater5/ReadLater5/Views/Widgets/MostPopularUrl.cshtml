@model Dictionary<string, string>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <style>
        body { font-family: Arial, sans-serif; margin: 0; padding: 0; font-size:14px; }
        .container { padding: 10px; border: 1px solid #ccc; width: 250px; background: #f9f9f9; }
        .title { font-size: 16px; margin-bottom: 8px; }
        .url { margin-bottom: 5px; }
        .clicks { font-size: 12px; color: #666; }
    </style>
</head>
<body>
<div class="container">
    <div class="title">Most Popular URL (All Time)</div>
    @if (Model.Count > 0)
    {
        <div class="url"><a href="@Model["url"]" target="_blank">@Model["url"]</a></div>
        <div class="clicks">Total Clicks: @Model["clicks"]</div>
    }
    else
    {
        <div>No URL found</div>
    }
</div>
</body>
</html>